alias dcu='sudo docker-compose up'
alias dcud='sudo docker-compose up -d'
alias dcd='sudo docker-compose down'
alias dcdv='docker-compose down -v'

alias ds='sudo systemctl start docker'


#one time
#docker pull amazon/dynamodb-local

alias drdy='sudo docker run -p 8000:8000 amazon/dynamodb-local'
alias drdys='sudo docker run --name dynamodb-shared -p 8002:8000 amazon/dynamodb-local -sharedDb -dbPath .'


alias dcls='sudo docker container ls'
alias dcst='sudo docker container stop'

