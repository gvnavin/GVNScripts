function nvr {
    npm run init && npm run build && npm run lerna:version && npm run release && spd-say "nvr done"
}


alias ni='npm run init' 
alias nb='npm run build'
alias nr='npm run release'
alias ns='npm run start'

alias nsb='NODE_OPTIONS=--openssl-legacy-provider npm run storybook'

alias nud='npm run upgrade_deps'

alias nib='npm run init && npm run build'

alias nlv='npm run lerna:version'  
